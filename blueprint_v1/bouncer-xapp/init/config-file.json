{
    "name": "bouncerxapp",
    "version": "1.0.0",
    "containers": [
        {
            "name": "bouncerxappcontainer",
            "image": {
                "registry": "127.0.0.1:5001",
                "name": "bouncerxapp",
                "tag": "1.0.0"
            }
        }
    ],
    "messaging": {
        "ports": [
            {
                "name": "http",
                "container": "bouncerxappcontainer",
                "port": 8080,
                "description": "http service"
            },
            {
                "name": "rmrroute",
                "container": "bouncerxappcontainer",
                "port": 4561,
                "description": "rmr route port for bouncer xapp"
            },
            {
                "name": "rmrdata",
                "container": "bouncerxappcontainer",
                "port": 4560,
                "rxMessages": ["RIC_SUB_RESP", "RIC_INDICATION","RIC_SUB_DEL_RESP"],
                "txMessages": ["RIC_SUB_REQ","RIC_SUB_DEL_REQ"],
                "policies": [1],
                "description": "rmr data port for bouncer xapp"
            }
        ]
    },
    "rmr": {
        "protPort": "tcp:4560",
        "maxSize": 2072,
        "numWorkers": 1,
        "txMessages": [
            "RIC_SUB_REQ",
            "RIC_SUB_DEL_REQ"
        ],
        "rxMessages": [
            "RIC_SUB_RESP",
            "RIC_INDICATION",
            "RIC_SUB_DEL_RESP"
        ],
    "policies": [1]
    },
    "http":{
        "protPort": "tcp:8080"
    },
    "controls": {
            "fileStorage": false
    },
    "db" : {
            "waitForSdl": true
    }
}
