{
    "name": "kpmxapp",
    "version": "1.0.0",
    "containers": [
        {
            "name": "kpmxapp",
            "image": {
                "registry": "127.0.0.1:5001",
                "name": "kpmxapp",
                "tag": "1.0.0"
            }
        }
    ],
    "messaging": {
        "ports": [
            {
                "name": "http",
                "container": "kpmxapp",
                "port": 8080,
                "description": "http service"
            },
            {
                "name": "rmrroute",
                "container": "kpmxapp",
                "port": 4561,
                "description": "rmr route port for kpmxapp xapp"
            },
            {
                "name": "rmrdata",
                "container": "kpmxapp",
                "port": 4560,
                "rxMessages": ["RIC_SUB_RESP", "RIC_SUB_FAILURE", "RIC_SUB_DEL_RESP", "RIC_INDICATION", "ACT_XAPP_REQ"],
                "txMessages": ["RIC_SUB_REQ", "RIC_SUB_DEL_REQ", "RIC_SGNB_ADDITION_REQ", "RIC_SGNB_ADDITION_ACK", "REACT_XAPP_ACK"],
                "mtypes" : [
                        {"name":"ACT_XAPP_REQ","id":30000},
                        {"name":"REACT_XAPP_ACK","id":30001}
                ],
                "policies": [1],
                "description": "rmr data port for kpmxapp"
            }
        ]
    },
    "rmr": {
        "protPort": "tcp:4560",
        "maxSize": 2072,
        "numWorkers": 1,
        "txMessages": [
            "RIC_SUB_REQ", "A1_POLICY_RESP", "A1_POLICY_QUERY", "RIC_HEALTH_CHECK_RESP",
            "REACT_XAPP_ACK"
        ],
        "rxMessages": [
            "RIC_SUB_RESP",
            "A1_POLICY_REQ", "RIC_HEALTH_CHECK_REQ",
            "ACT_XAPP_REQ"
        ],
    "policies": [1]
    },
    "controls": {
            "fileStrorage": false
    },
    "db" : {
            "waitForSdl": false
    }
}
